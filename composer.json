{
    "name": "elao/admin-bundle",
    "type": "symfony-bundle",
    "description": "Simple, powerful and extendable admin bundle.",
    "keywords": ["elao", "admin", "crud"],
    "homepage": "https://github.com/Elao/ElaoAdminBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Elao",
            "homepage": "http://www.elao.com"
        },
        {
            "name": "Thomas Jarrand",
            "homepage": "https://github.com/Tom32i"
        },
        {
            "name": "Emeric Kasbarian",
            "homepage": "https://github.com/emerick42"
        }
    ],
    "require": {
        "symfony/framework-bundle": "~3.1",
        "doctrine/inflector": "~1.1"
    },
    "suggest": {
        "doctrine/orm": "For using the built-in DoctrineModelManager",
        "elao/html-action-bundle": "For easily performing CRUD operations using Symfony forms",
        "elao/rest-action-bundle": "For building an Api through REST actions"
    },
    "conflict": {
        "doctrine/orm": "<2.5"
    },
    "autoload": {
        "psr-4": {
            "Elao\\Bundle\\AdminBundle\\": "."
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    }
}
